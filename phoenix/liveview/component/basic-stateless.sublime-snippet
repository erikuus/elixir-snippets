use $TM_NAMESPACE_WEB, :live_component

def mount(socket) do
  {:ok, socket}
end

def update(assigns, socket) do
  socket = assign(socket, assigns)
  {:ok, socket}
end

def render(assigns) do
  ~H"""
  <div>
      
  </div>
  """
end
